print("Birinci Insanin Dogum Tarixini Daxil Edin")
a = int(input("Doguldugu Gun:"))
b = int(input("Ay:"))
c = int(input("İl:"))
print("Ikinci Insanin Dogum Tarixini Daxil Edin")
d = int(input("Doguldugu Gun:"))
e = int(input("Ay"))
f = int(input("İl:"))
if c < 2020 and c > 1800 and c > f:
    print("Boyukdur:", d, ".", e, ".", f)
elif c < 2020 and c > 1800 and c == f:
    if b < 12 and b > 0 and b > e:
        print("Boyukdur:", d, ".", e, ".", f)
    elif b < 12 and b > 0 and b < e:
        print("Boyukdur:", a, ".", b, ".", c)
    elif b < 12 and b > 0 and b == e:
        if a < 31 and a > 0 and a > d:
            print("Boyukdur:", d, ".", e, ".", f)
        elif a < 31 and a > 0 and a < d:
            print("Boyukdur:", a, ".", b, ".", c)
        elif a < 31 and a > 0 and a == d:
            print("her ikisi eyni yasdadir.")
elif c < 2020 and c > 1800 and c > f:
    print("Boyukdur:","a","b","c",)
